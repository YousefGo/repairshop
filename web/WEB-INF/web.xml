<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RepairRequest</servlet-name>
        <servlet-class>RepairRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFeedback</servlet-name>
        <servlet-class>AddFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newRequset</servlet-name>
        <servlet-class>newRequset</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>allRepairRequest</servlet-name>
        <servlet-class>allRepairRequest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RepairRequest</servlet-name>
        <url-pattern>/AddRepairRequst</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFeedback</servlet-name>
        <url-pattern>/AddFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newRequset</servlet-name>
        <url-pattern>/newRequset</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>allRepairRequest</servlet-name>
        <url-pattern>/allRepairRequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            15
        </session-timeout>
    </session-config>
</web-app>
